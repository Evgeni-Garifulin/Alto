							a.link-secondary-md(href='#') Услуги
						li.header__menu-item.pos-relative
							a.link-secondary-md(href='#') Акции
							.badge.header__menu-item-counter.pos-absolute.py-2.px-5 6
				form.header__search.pos-relative.mr-45(action='#')
					input.header__search-field.w-100p.pl-15(name='s' type='search' placeholder='Поиск' autocomplete='off' aria-label='Поиск по товарам')
					svg.header__search-field-icon.pos-absolute.d-block(width=15 height=15)
						use(xlink:href='/img/svg/icons.svg#loupe')
				.header__basket.pos-relative.d-flex.flex-ai-center.flex-jc-center.mr-30
